{
  "template": {
    "name": "Web Application Template",
    "version": "1.0.0",
    "description": "Universal template for web applications with modern stack",
    "type": "web",
    "category": "frontend",
    "tags": ["react", "typescript", "nodejs", "express", "mongodb"],
    "author": "ManagerAgentAI",
    "created": "2025-01-31",
    "lastUpdated": "2025-01-31"
  },
  "structure": {
    "directories": [
      "src",
      "src/components",
      "src/pages",
      "src/hooks",
      "src/utils",
      "src/services",
      "src/types",
      "src/assets",
      "src/assets/images",
      "src/assets/styles",
      "public",
      "tests",
      "tests/unit",
      "tests/integration",
      "docs",
      "scripts",
      "config"
    ],
    "files": [
      "package.json",
      "tsconfig.json",
      "webpack.config.js",
      "babel.config.js",
      ".env.example",
      ".gitignore",
      "README.md",
      "src/index.tsx",
      "src/App.tsx",
      "src/components/Header.tsx",
      "src/components/Footer.tsx",
      "src/pages/Home.tsx",
      "src/pages/About.tsx",
      "src/hooks/useLocalStorage.ts",
      "src/utils/helpers.ts",
      "src/services/api.ts",
      "src/types/index.ts",
      "src/assets/styles/global.css",
      "src/assets/styles/components.css",
      "public/index.html",
      "tests/setup.ts",
      "tests/App.test.tsx",
      "docs/API.md",
      "scripts/build.js",
      "scripts/dev.js",
      "config/database.js"
    ]
  },
  "dependencies": {
    "production": [
      "react",
      "react-dom",
      "typescript",
      "@types/react",
      "@types/react-dom",
      "express",
      "cors",
      "helmet",
      "morgan",
      "mongoose",
      "dotenv"
    ],
    "development": [
      "@testing-library/react",
      "@testing-library/jest-dom",
      "jest",
      "webpack",
      "webpack-cli",
      "webpack-dev-server",
      "babel-loader",
      "@babel/core",
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-typescript",
      "css-loader",
      "style-loader",
      "html-webpack-plugin",
      "eslint",
      "@typescript-eslint/parser",
      "@typescript-eslint/eslint-plugin",
      "prettier"
    ]
  },
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
    "start": "node dist/server.js"
  },
  "configuration": {
    "port": 3000,
    "apiPort": 3001,
    "database": "mongodb://localhost:27017/webapp",
    "environment": "development"
  },
  "features": {
    "routing": true,
    "stateManagement": "context",
    "testing": true,
    "linting": true,
    "formatting": true,
    "hotReload": true,
    "pwa": false,
    "ssr": false,
    "typescript": true,
    "scss": false,
    "cssModules": false
  }
}
