{
  "template": {
    "name": "Mobile Application Template",
    "version": "1.0.0",
    "description": "Universal template for mobile applications with React Native",
    "type": "mobile",
    "category": "mobile",
    "tags": ["react-native", "typescript", "expo", "redux", "navigation"],
    "author": "ManagerAgentAI",
    "created": "2025-01-31",
    "lastUpdated": "2025-01-31"
  },
  "structure": {
    "directories": [
      "src",
      "src/components",
      "src/screens",
      "src/navigation",
      "src/services",
      "src/store",
      "src/utils",
      "src/types",
      "src/assets",
      "src/assets/images",
      "src/assets/fonts",
      "src/assets/icons",
      "tests",
      "tests/unit",
      "tests/integration",
      "docs",
      "scripts",
      "config",
      "android",
      "ios"
    ],
    "files": [
      "package.json",
      "tsconfig.json",
      "babel.config.js",
      "metro.config.js",
      "app.json",
      ".env.example",
      ".gitignore",
      "README.md",
      "src/App.tsx",
      "src/components/Button.tsx",
      "src/components/Input.tsx",
      "src/screens/HomeScreen.tsx",
      "src/screens/ProfileScreen.tsx",
      "src/navigation/AppNavigator.tsx",
      "src/navigation/TabNavigator.tsx",
      "src/services/api.ts",
      "src/services/storage.ts",
      "src/store/index.ts",
      "src/store/slices/userSlice.ts",
      "src/utils/helpers.ts",
      "src/types/index.ts",
      "tests/setup.ts",
      "tests/App.test.tsx",
      "docs/API.md",
      "scripts/build.js",
      "scripts/dev.js",
      "config/database.js"
    ]
  },
  "dependencies": {
    "production": [
      "react-native",
      "expo",
      "typescript",
      "@types/react",
      "@types/react-native",
      "@react-navigation/native",
      "@react-navigation/stack",
      "@react-navigation/bottom-tabs",
      "react-redux",
      "@reduxjs/toolkit",
      "react-native-async-storage",
      "react-native-vector-icons",
      "expo-linear-gradient",
      "expo-status-bar"
    ],
    "development": [
      "@testing-library/react-native",
      "@testing-library/jest-native",
      "jest",
      "metro",
      "metro-react-native-babel-preset",
      "eslint",
      "@typescript-eslint/parser",
      "@typescript-eslint/eslint-plugin",
      "prettier",
      "react-native-debugger"
    ]
  },
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "configuration": {
    "platform": "expo",
    "sdkVersion": "49.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": "./assets/splash.png"
  },
  "features": {
    "navigation": true,
    "stateManagement": "redux",
    "testing": true,
    "linting": true,
    "formatting": true,
    "hotReload": true,
    "typescript": true,
    "vectorIcons": true,
    "asyncStorage": true,
    "gradients": true,
    "statusBar": true,
    "android": true,
    "ios": true,
    "web": true
  }
}
