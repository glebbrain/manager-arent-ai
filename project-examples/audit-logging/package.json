{
  "name": "audit-logging",
  "version": "2.5.0",
  "description": "Comprehensive Audit Logging System with advanced analytics, compliance reporting, and security monitoring",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "security-scan": "npm audit && bandit -r . -f json",
    "build": "echo 'No build step required'",
    "docker:build": "docker build -t audit-logging .",
    "docker:run": "docker run -p 3013:3013 audit-logging"
  },
  "keywords": [
    "audit",
    "logging",
    "compliance",
    "security",
    "monitoring",
    "analytics",
    "reporting",
    "gdpr",
    "hipaa",
    "soc2"
  ],
