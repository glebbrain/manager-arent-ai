{
  "name": "compliance-framework",
  "version": "2.5.0",
  "description": "Comprehensive Compliance Framework for GDPR, HIPAA, SOC2, and PCI-DSS compliance management",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "lint": "eslint .",
    "security-scan": "npm audit && bandit -r . -f json",
    "build": "echo 'No build step required'",
    "docker:build": "docker build -t compliance-framework .",
    "docker:run": "docker run -p 3010:3010 compliance-framework"
  },
  "keywords": [
    "compliance",
    "gdpr",
    "hipaa",
    "soc2",
    "pci-dss",
    "governance",
    "audit",
    "regulatory",
    "data-protection"
  ],
  "author": "Universal Automation Platform",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-plugin-security": "^1.7.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/universal-automation-platform/compliance-framework.git"
  },
  "bugs": {
    "url": "https://github.com/universal-automation-platform/compliance-framework/issues"
  },
  "homepage": "https://github.com/universal-automation-platform/compliance-framework#readme"
}
