{
  "version": "3.5.0",
  "lastUpdated": "2025-01-31",
  "project": {
    "name": "ManagerAgentAI",
    "type": "universal",
    "environment": "development"
  },
  "features": {
    "ai": {
      "enabled": true,
      "models": ["GPT-4o", "Claude-3.5", "Gemini 2.0"],
      "capabilities": ["code-analysis", "predictive-analytics", "optimization"]
    },
    "quantum": {
      "enabled": false,
      "frameworks": ["Qiskit", "Cirq", "PennyLane"],
      "capabilities": ["VQE", "QAOA", "Grover Search"]
    },
    "enterprise": {
      "enabled": true,
      "cloudProviders": ["AWS", "Azure", "GCP"],
      "capabilities": ["multi-cloud", "serverless", "edge-computing"]
    },
    "uiux": {
      "enabled": true,
      "tools": ["Figma", "Sketch", "Adobe XD"],
      "capabilities": ["wireframes", "prototypes", "responsive-design"]
    },
    "advanced": {
      "enabled": true,
      "capabilities": ["performance-optimization", "security-analysis", "monitoring"]
    }
  },
  "automation": {
    "parallelExecution": true,
    "maxConcurrentTasks": 4,
    "timeout": 300,
    "retryAttempts": 3,
    "logLevel": "INFO"
  },
  "paths": {
    "scripts": ".automation",
    "reports": ".manager/reports",
    "logs": ".manager/logs",
    "backups": "backups",
    "temp": ".temp"
  },
  "notifications": {
    "enabled": true,
    "email": false,
    "slack": false,
    "webhook": false
  }
}
